:@template"£"@import "compass";

$base-background-color:#fff;
$base-font-color:#000;
$base-link-color:#369;

body{
    margin: 0 auto;
    padding: 0;
    max-width: 1020px;
    font-size: 16px;
    color: #444;
    background-color:#f4f4f4;
}
* {
    @include box-sizing(border-box);
}


@mixin grid-item-columns($columns) {
    width: (100% / 12) * $columns;
}
.refills-wrapper{
    &:after{
        content: " ";
        display: block;
        width: 100%;
        height: 10px;
        background-color: white;
        margin-top: -2px;
        position: absolute;
        z-index: 1;
    }
}
.grid-items-lines {
    $grid-items-background: $base-background-color;
    $grid-item-background: $base-background-color;
    $grid-item-border: 1px solid transparentize($base-font-color, .8);
    $grid-item-columns: 4;
    $grid-item-big-columns: 8;
    $grid-item-color: $base-font-color;
    $grid-item-height: 14em;
    @include background-image(linear-gradient(#ddd, #ddd), linear-gradient(#ddd, #ddd));
    @include background-size(1px 100%, 1px 100%);
    @include clearfix;
    background-repeat: no-repeat, no-repeat;
    background-position:  33.333% 0, 66.66% 0;
    background-color: #FFF;
    &:after {
        @extend clearfix;
        content: "";
        display: table;
        clear: both;
}
.grid-item {
    @include transition (all 0.2s ease-in-out);
    height: $grid-item-height;
    width:  33.333%;
    overflow: hidden;
    border-bottom: $grid-item-border;
    float: left;
    padding: 0.2em 1.3em;
    display: block;
    position: relative;
    &:hover {
        small{
           @include opacity(1);
           width: auto;
        }
    }


    img {
        display: block;
        margin-bottom: 1.2em;
    }

    h1 {
        color: #369;
        font-size: 1.5em;
        margin-bottom: .2em;
        font-family: 'Radley', sans-serif;
    }
    small{
        color: #888;
        font-size: .66em;
        background-color: #ddd;
        padding: .3em .6em;
        position:absolute;
        right: 0;
        top: 2px;
        font-family: 'Radley', sans-serif;
        text-transform: uppercase;
        @include opacity(.1);
        @include transition (all 0.2s ease-in-out);
        width: 0;
    }
    p {
        color: transparentize($grid-item-color, .4);
        font-family: 'Noto Sans', sans-serif;
        font-size: .85em;

    }


    }
    .grid-item-big {
        width:66.66%;
        background-color:  $base-background-color;
        border-left: 1px solid #ddd;
        height: 32em;
    }
    .grid-last-big{ height: 32em}
    .grid-item-image{

        padding: 0;
        height: 13em;
        position: relative;
        &:hover{
            span{ @include opacity(1); height: 40px}
            a{
                line-height:3em;
                @include background-image(linear-gradient(rgba(0,0,0,0), rgba(0,0,0,.4)));
            }
        }
        img{
            display: block;
        }
        a {
            @include transition (all 0.2s ease-in-out);
            position: absolute;
            bottom: -2px;
            left: 0px;
            right: 0px;
            padding: 2px 0px;
            color: #ffffff;
            font-size: 1.3em;
            line-height: 1px;
            text-align: center;
            text-decoration: none;
            font-family: 'Radley', sans-serif;
            @include text-shadow(0 1px 1px rgba(0,0,0,.7));
            @include background-image(linear-gradient(rgba(0,0,0,0), rgba(0,0,0,.4)));

        }
        span{
            @include transition (all 0.2s ease-in-out);
            @include opacity(0);
            font-size: .8rem;
            background-color: rgba(0,51,102,.8);
            display:block;
            width: 100%;
            line-height: 40px;
            font-family: 'Radley', sans-serif;
        }
    }
}



@mixin font($font-size,$line-height,$font-family,$font-weight) {
    font:#{$font-size}/#{$line-height} $font-family;
    font-weight:$font-weight;
}

@mixin keyframe ($animation_name) {
    @-webkit-keyframes $animation_name {
        @content;
    }

    @-moz-keyframes $animation_name {
        @content;
    }

    @-o-keyframes $animation_name {
        @content;
    }

    @keyframes $animation_name {
        @content;
    }
}

@mixin animation ($delay, $duration, $animation) {
    -webkit-animation-delay: $delay;
    -webkit-animation-duration: $duration;
    -webkit-animation-name: $animation;
    -webkit-animation-fill-mode: forwards;

    -moz-animation-delay: $delay;
    -moz-animation-duration: $duration;
    -moz-animation-name: $animation;
    -moz-animation-fill-mode: forwards;

    -o-animation-delay: $delay;
    -o-animation-duration: $duration;
    -o-animation-name: $animation;
    -o-animation-fill-mode: forwards;

    animation-delay: $delay;
    animation-duration: $duration;
    animation-name: $animation;
    animation-fill-mode: forwards;
}

@mixin breakpoint($point) {
    @if $point == big {
        @media screen and (max-width: 1600px){@content;}
    }
    @else if $point == medium {
        @media screen and (max-width: 900px){@content;}
    }
    @else if $point == small {
        @media screen and (max-width: 600px){@content;}
    }
}

@include keyframe(content-opacity) {
    from {
        opacity:0;
    }
    to {
        opacity:1;
    }
}

@include keyframe(content-rotate-y) {
    from {
        @include transform(rotateY(90deg));
    }
    to {
        opacity:1;
        @include transform(rotateY(0deg));
    }
}

@include keyframe(content-rotate-x) {
    from {
        @include transform(rotateX(90deg));
    }
    to {
        opacity:1;
        @include transform(rotateX(0deg));
    }
}

@include keyframe(content-rotate-both) {
    from {
        @include transform(rotate(90deg));
        @include transform-origin(0% 50%);
    }
    to {
        opacity:1;
        @include transform(rotate(0deg));
        @include transform-origin(0% 50%);
    }
}

@include keyframe(content-pop-out) {
    0% {
        opacity:1;
        @include transform(scale(1));
    }
    50% {
        opacity:1;
        @include transform(scale(1.1));
    }
    100% {
        opacity:1;
        @include transform(scale(1));
    }
}

@include keyframe(content-pop-in) {
    from {
        opacity:1;
        @include transform(scale(1.1));
    }
    to {
        opacity:1;
        @include transform(scale(1));
    }
}

@include keyframe(content-slide-bot) {
    from {
        top:20px;
        opacity:0;
    }
    to {
        top:0px;
        opacity:1;
    }
}

@include keyframe(content-slide-top) {
    from {
        top:-20px;
        opacity:0;
    }
    to {
        top:0px;
        opacity:1;
    }
}

/* used for wrapper animation after the load of the page */
@include keyframe(show) {
    from {
        opacity:0;
    }
    to {
        opacity:1;
    }
}


.tabs-wrapper {
    font-family: 'Radley', sans-serif;
    margin:0;
    height: 28em;
    color: $base-link-color;
    width: 110%;
    font-size: 1em;
    position: relative;
    left: -6px;
    input[type=radio] {
        display:none;
    }
    label {
        transition:background 0.4s ease-in-out, height 0.2s linear;
        cursor:pointer;
        width:20%;
        height:2.4em;
        text-align:center;
        line-height:2.4em;
        color: #999;
        padding: 0 0 0 5px;
        &:last-of-type {
            border-bottom:none;
        }
        &:hover {
            color: $base-link-color;
        }
        @include breakpoint(big){width:24%;}
        @include breakpoint(medium){width:20%;}
        @include breakpoint(small){
        width:100%;
        display:block;
        border-radius:0;
    }
    }
    @include breakpoint(small){margin:0;}
}

#tab1, #tab2, #tab3, #tab4 {
    &:checked{
        + label {
            background:#fff;
            color:$base-link-color;
            border-bottom:1px solid $base-link-color;
        }
    }
}

.tab-body {
    position:absolute;
    top:-9999px;
    opacity:0;
    padding:10px;
}

.tab-body-wrapper {
    width: 94%;
    @include animation(0.2s,1.5s,show);
    @include breakpoint(small){
    border:1px solid red;
  }
}

#tab1:checked ~ .tab-body-wrapper #tab-body-1, #tab2:checked ~ .tab-body-wrapper #tab-body-2 , #tab3:checked ~ .tab-body-wrapper #tab-body-3 , #tab4:checked ~ .tab-body-wrapper  #tab-body-4 {
    position:relative;
    top:0px;
    /* Choose one */
     // @include animation(0.2s,0.8s,content-opacity);
    // @include animation(0.2s,0.8s,content-rotate-x);
    @include animation(0.2s,0.8s,content-rotate-y);
    // @include animation(0.2s,0.8s,content-rotate-both);
    /* @include animation(0.2s,0.8s,content-pop-in); */
    /* @include animation(0.2s,0.8s,content-pop-out); */
    // @include animation(0.1s,1s,content-slide-bot);
    // @include animation(0.1s,1s,content-slide-top);
}
